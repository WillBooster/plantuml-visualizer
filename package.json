{
  "name": "plantuml-visualizer",
  "version": "1.3.0",
  "private": true,
  "description": "A Chrome / Firefox extension for visualizing PlantUML code.",
  "homepage": "https://github.com/WillBooster/plantuml-visualizer",
  "bugs": {
    "url": "https://github.com/WillBooster/plantuml-visualizer/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WillBooster/plantuml-visualizer.git"
  },
  "license": "Apache-2.0",
  "author": "WillBooster LLC",
  "main": "index.js",
  "scripts": {
    "build": "yarn build-dev",
    "build-common": "yarn recreate-dist && rollup -c",
    "build-dev": "cross-env NODE_ENV=development yarn build-common",
    "build-prod": "cross-env NODE_ENV=production yarn build-common",
    "format": "yarn lint-fix && yarn prettier && yarn sort-package-json",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "yarn lint --fix",
    "package": "shx rm -f plantuml-visualizer-v*.tgz dist-*.zip && yarn pack && yarn build-prod && cd dist && bestzip ../dist.zip *",
    "prettier": "prettier --write {,*/}**/*.{css,htm,html,js,json,md,scss,yaml,yml}",
    "recreate-dist": "shx rm -Rf dist && shx mkdir dist && shx cp -r icon manifest.json dist/",
    "typecheck": "tsc --noEmit",
    "watch": "yarn build -w"
  },
  "prettier": "@willbooster/prettier-config",
  "dependencies": {
    "jquery": "3.4.1",
    "tslib": "1.10.0",
    "zlib.es": "0.6.0"
  },
  "devDependencies": {
    "@types/chrome": "0.0.89",
    "@types/jquery": "3.3.31",
    "@typescript-eslint/eslint-plugin": "2.3.1",
    "@typescript-eslint/parser": "2.3.1",
    "@willbooster/eslint-config": "1.0.0",
    "@willbooster/prettier-config": "1.0.0",
    "@willbooster/tsconfig": "1.0.0",
    "bestzip": "2.1.4",
    "cross-env": "6.0.0",
    "eslint": "6.5.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.1",
    "husky": "3.0.7",
    "lint-staged": "9.4.0",
    "prettier": "1.18.2",
    "rollup": "1.22.0",
    "rollup-plugin-closure-compile": "1.1.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-typescript2": "0.24.3",
    "shx": "0.3.2",
    "sort-package-json": "1.22.1",
    "typescript": "3.6.3"
  }
}
